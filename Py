import pandas as pd

# Read the previous output Excel file
previous_output_df = pd.read_excel('output.xlsx')

# Select the "Counterparty Name" column
counterparty_names = previous_output_df['counterparty names']

# Create a DataFrame with the "Counterparty Name" column
new_df = pd.DataFrame({'Counterparty Name': counterparty_names})

# Add new columns vd, vd1, vd2, ..., vd7
for i in range(8):
    new_df[f'vd{i}'] = ''

# Write the DataFrame to a new CSV file
new_df.to_csv('new_output.csv', index=False)
