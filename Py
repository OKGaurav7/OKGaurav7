import pandas as pd

# Read the output Excel file
output_df = pd.read_excel('output.xlsx')

# Select only the 'Counterparty Name' column
counterparty_df = output_df[['counterparty names']]

# Create a new DataFrame with the 'Counterparty Name' column and empty columns vd, vd1, vd2, vd3, vd4, vd5, vd6, vd7
new_df = counterparty_df.assign(vd='', vd1='', vd2='', vd3='', vd4='', vd5='', vd6='', vd7='')

# Write the new DataFrame to a CSV file
new_df.to_csv('new_output.csv', index=False)
