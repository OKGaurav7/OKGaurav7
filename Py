import win32com.client

# Path to the Excel file containing the macro
excel_file_path = "path/to/ab.xlsm"

# Path to the macro file
macro_file_path = "path/to/idk.bas"

# Create a new instance of Excel application
excel = win32com.client.Dispatch("Excel.Application")

# Open the Excel workbook
workbook = excel.Workbooks.Open(excel_file_path)

# Run the macro from the external file
excel.Run(macro_file_path + "!ModuleName.MacroName")

# Save and close the workbook
workbook.Save()
workbook.Close()

# Quit Excel application
excel.Quit()
